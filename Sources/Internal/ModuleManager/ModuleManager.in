#include "ModuleManager/ModuleManager.h"
#include "ModuleManager/IModule.h"

@IMODULE_INCLUDES@

namespace DAVA
{
    ModuleManager::ModuleManager()
    {
@IMODULE_CODE@
    }

    ModuleManager::~ModuleManager()
    {
        for (const auto& module : listModules)
        {
            module->Shutdown();
            delete module;
        }
    }    
}